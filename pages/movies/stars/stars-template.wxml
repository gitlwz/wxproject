<template name="starsTemplate">
    <view class='stars-container'>
        <view class='stars'>
            <block wx:for="{{stars}}" wx:for-item="star">
                <image src="{{star == 1?'/images/icon/star.png':'/images/icon/none-star.png'}}"></image>
                
            </block>
        </view>
        
        <text>{{average}}</text>
    </view>
</template>